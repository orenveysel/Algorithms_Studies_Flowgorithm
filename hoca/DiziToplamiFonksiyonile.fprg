    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="toplanacaklar" type="Integer" array="True" size="10"/>
            <declare name="i, j, sonuc" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <assign variable="toplanacaklar[i]" expression="Random(100)"/>
            </for>
            <assign variable="sonuc" expression="Topla(toplanacaklar,10)"/>

    <function name="Topla" type="Integer" variable="toplam">
        <parameters>
            <parameter name="dizi" type="Integer" array="True"/>
            <parameter name="elemansayisi" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, toplam" type="Integer" array="False" size=""/>
            <assign variable="toplam" expression="0"/>
            <for variable="i" start="0" end="elemansayisi -1" direction="inc" step="1">
                <assign variable="toplam" expression="toplam + dizi[i]"/>
            </for>
        </body>
    </function>
